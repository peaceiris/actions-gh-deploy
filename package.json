{
  "name": "actions-github-pages",
  "version": "3.6.1",
  "description": "GitHub Actions for GitHub Pages",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12.16.3",
    "npm": ">=6.14.4"
  },
  "scripts": {
    "all": "npm run format && npm run lint && npm test",
    "lint": "eslint ./{src,__tests__}/**/*.ts",
    "lint:fix": "eslint --fix ./{src,__tests__}/**/*.ts",
    "test": "jest --coverage --verbose --detectOpenHandles",
    "build": "ncc build ./src/index.ts -o lib --minify",
    "tsc": "tsc",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "release": "standard-version",
    "update-deps": "(git diff 'HEAD@{1}' --name-only | grep 'package-lock.json' > /dev/null) && npm ci || :"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "npm run update-deps; git remote prune origin"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --check",
      "eslint"
    ],
    "README.md": [
      "npx doctoc@1.4.0 --github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peaceiris/actions-gh-pages.git"
  },
  "keywords": [
    "GitHub Actions",
    "Actions",
    "JavaScript Action",
    "TypeScript Action",
    "GitHub Pages",
    "gh-pages"
  ],
  "author": "peaceiris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peaceiris/actions-gh-pages/issues"
  },
  "homepage": "https://github.com/peaceiris/actions-gh-pages#readme",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^2.1.1",
    "@actions/io": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/js-yaml": "^3.12.3",
    "@types/node": "~12",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "@zeit/ncc": "^0.22.1",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.9.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "jest-circus": "^26.0.1",
    "js-yaml": "^3.13.1",
    "lint-staged": "^10.2.2",
    "prettier": "2.0.5",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.5.0",
    "typescript": "^3.8.3"
  }
}
